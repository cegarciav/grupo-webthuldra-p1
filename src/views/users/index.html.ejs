<main>
  <section class="sn-users-feed">
    <% usersList.forEach(user => { %>
      <article class="user">
        <figure class="foto-usuario-post">
          <a href="<%= userPath(user.id) %>">
            <img src="<%= user.picture? user.picture: assetPath('default_profile_picture.png') %>">
          </a>
          <figcaption class="info-usuario">
            <a href="<%= userPath(user.id) %>">
              <h1><%= user.firstName + " " + user.lastName %> </h1>
            </a>
          </figcaption>
        </figure>
      </article>
    <% }); %>
  </section>
  <section class="sn-pagination">
    <ul>
      <li><a href="<%= usersPath() %>">&laquo;</a></li>
      <% pagesArray.forEach(page => { %>
        <li><a href="<%= usersPath(page) %>"><%= page %></a></li>
      <% }); %>
      <li><a href="<%= usersPath(pagesAmount) %>">&raquo;</a></li>
    </ul>
  </section>
</main>
